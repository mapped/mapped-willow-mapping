{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePatterns": [ ".*" ],
      "enabled": false 
    },
    {
      "matchPackagePatterns": [ "Mapped\\.Ontologies\\.Core\\.Dtdl"],
      "automerge": true,
      "labels": ["renovate", "auto"],
      "commitMessage": "Update Mapped Ontologies DTDL package",
      "prBodyColumns": [
        "Package",
        "Change"
      ],
      "enabled": true
    },
    {
      "matchPackagePatterns": ["WillowInc\\.Ontology\\.DTDLv3" ],
      "automerge": true,
      "labels": ["renovate", "auto"],
      "commitMessage": "Update WillowInc Ontology DTDLv3 package",
      "prBodyColumns": [
        "Package",
        "Change"
      ],
      "enabled": true
    }
  ]
}
